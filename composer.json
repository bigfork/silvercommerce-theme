{
    "name": "silvercommerce/silvercommerce-theme",
    "description": "A simple SilverStripe theme built on bootstrap 4",
    "type": "silverstripe-theme",
    "keywords": [
        "silverstripe",
        "theme",
        "bootstrap4"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Morven Lewis-Everley",
            "homepage": "http://www.ilateral.co.uk"
        }
    ],
    "extra": {
        "installer-name": "silvercommerce",
        "expose": [
            "css",
            "javascript",
            "images",
            "icons",
            "fonts"
        ]
    },
    "require": {
        "composer/installers": "*",
        "silvercommerce/catalogue-admin": "dev-ss5",
        "silvercommerce/catalogue-frontend": "dev-ss5"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-catalogue-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-catalogue-frontend"
        }
    ]
}
